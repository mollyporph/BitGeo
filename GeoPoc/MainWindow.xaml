<Window x:Class="GeoPoc.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GeoPoc"
        mc:Ignorable="d"
        Title="GeoPoc" Height="720" Width="1280">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="60*"/>
            <ColumnDefinition Width="40*"/>
        </Grid.ColumnDefinitions>
        <Canvas Grid.Column="0" Name="mapCanvas" HorizontalAlignment="Left" Background="Black" Height="700" Width="760" MouseLeftButtonUp="mapCanvas_MouseLeftButtonUp"></Canvas>
        <TextBlock Grid.Column="1" Name="Data"></TextBlock>
    </Grid>
</Window>
